<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:layout_width="match_parent"
    android:layout_height="match_parent" xmlns:tools="http://schemas.android.com/tools"
    android:layout_marginLeft="10dp"
    android:orientation="vertical" android:layout_marginStart="10dp"

    >
    <Button
        android:layout_width="54.7dp"
        android:layout_height="35.5dp"
        android:layout_alignParentRight="true"
        android:textSize="7.3sp"
        android:fontFamily="sans-serif-medium"
        android:textStyle="normal"
        android:textColor="#15e6b6"
        android:background="#00000000"
        android:letterSpacing="0.01"
        android:lineSpacingExtra="42.7sp"
        android:gravity="center"
        android:text="SKIP"
        />

    <TextView
        android:id="@+id/wlf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="16.7sp"
        android:textColor="#454545"
        android:fontFamily="sans-serif-medium"
        android:textStyle="bold"
        android:text="What are you looking for?"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"
        />
    <TextView
        android:id="@+id/intro"
        android:layout_width="188dp"
        android:layout_height="wrap_content"
        android:textSize="8.7sp"
        android:fontFamily="sans-serif"
        android:textStyle="normal"
        android:textColor="#454545"
        android:lineSpacingExtra="4.7sp"
        android:layout_centerHorizontal="true"
        android:text="We will help you find farmers markets near you with the products you like! If you’re not sure, come back and tell us what you’re looking for after you’ve looked around."
        android:layout_below="@id/wlf"
        />
    <TextView
        android:id="@+id/firR"
        android:layout_width="77.8dp"
        android:layout_height="29.7dp"
        android:textSize="8.7sp"
        android:fontFamily="sans-serif"
        android:textStyle="normal"
        android:textColor="#bcbcbc"
        android:lineSpacingExtra="41.3sp"
        android:gravity="center"
        android:text="Local Markets"
        android:layout_below="@id/intro"
        android:layout_alignStart="@+id/wlf"
        android:layout_alignLeft="@+id/wlf"
        android:layout_marginTop="10dp"
        android:background="#f4f4f4"
        />

    <TextView
        android:id="@+id/secR"
        android:layout_width="43.8dp"
        android:layout_height="29.7dp"
        android:textSize="8.7sp"
        android:fontFamily="sans-serif"
        android:textStyle="normal"
        android:textColor="#bcbcbc"
        android:lineSpacingExtra="41.3sp"
        android:gravity="center"
        android:text="Deals"
        android:layout_below="@id/firR"
        android:layout_alignStart="@+id/wlf"
        android:layout_alignLeft="@+id/wlf"
        android:layout_marginTop="4.3dp"
        android:background="#f4f4f4"
        />
    <TextView
        android:id="@+id/thiR"
        android:layout_width="43.8dp"
        android:layout_height="29.7dp"
        android:textSize="8.7sp"
        android:fontFamily="sans-serif"
        android:textStyle="normal"
        android:textColor="#bcbcbc"
        android:lineSpacingExtra="41.3sp"
        android:gravity="center"
        android:text="Herbs"
        android:layout_below="@id/secR"
        android:layout_alignStart="@+id/wlf"
        android:layout_alignLeft="@+id/wlf"
        android:layout_marginTop="4.3dp"
        android:background="#f4f4f4"
        />
    <TextView
        android:id="@+id/forR"
        android:layout_width="52.4dp"
        android:layout_height="29.7dp"
        android:textSize="8.7sp"
        android:fontFamily="sans-serif"
        android:textStyle="normal"
        android:textColor="#bcbcbc"
        android:lineSpacingExtra="41.3sp"
        android:gravity="center"
        android:text="Cheese"
        android:layout_below="@id/thiR"
        android:layout_alignStart="@+id/wlf"
        android:layout_alignLeft="@+id/wlf"
        android:layout_marginTop="4.3dp"
        android:background="#f4f4f4"
        />
    <TextView
        android:id="@+id/fivR"
        android:layout_width="62.1dp"
        android:layout_height="29.7dp"
        android:textSize="8.7sp"
        android:fontFamily="sans-serif"
        android:textStyle="normal"
        android:textColor="#bcbcbc"
        android:lineSpacingExtra="41.3sp"
        android:gravity="center"
        android:text="Jewelry"
        android:layout_below="@id/forR"
        android:layout_alignStart="@+id/wlf"
        android:layout_alignLeft="@+id/wlf"
        android:layout_marginTop="4.3dp"
        android:background="#f4f4f4"
        />
    <TextView
        android:id="@+id/sixR"
        android:layout_width="71.5dp"
        android:layout_height="29.7dp"
        android:textSize="8.7sp"
        android:fontFamily="sans-serif"
        android:textStyle="normal"
        android:textColor="#bcbcbc"
        android:lineSpacingExtra="41.3sp"
        android:gravity="center"
        android:text="Seasonings"
        android:layout_below="@id/fivR"
        android:layout_alignStart="@+id/wlf"
        android:layout_alignLeft="@+id/wlf"
        android:layout_marginTop="4.3dp"
        android:background="#f4f4f4"
        />
    <TextView
        android:id="@+id/sevR"
        android:layout_width="56.7dp"
        android:layout_height="29.7dp"
        android:textSize="8.7sp"
        android:fontFamily="sans-serif"
        android:textStyle="normal"
        android:textColor="#bcbcbc"
        android:lineSpacingExtra="41.3sp"
        android:gravity="center"
        android:text="Garden"
        android:layout_below="@id/sixR"
        android:layout_alignStart="@+id/wlf"
        android:layout_alignLeft="@+id/wlf"
        android:layout_marginTop="4.3dp"
        android:background="#f4f4f4"
        />


</RelativeLayout>